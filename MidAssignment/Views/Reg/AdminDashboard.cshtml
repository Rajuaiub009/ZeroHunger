
@{
    ViewBag.Title = "AdminDashboard";
}

<h2>Admin Dashboard</h2>

<a href="/Reg/AdminDashboard">Requests</a>
<a href="/Reg/AdminHistory"> History </a>
<a href="/Reg/ResAdmin"> Restaurants </a>
<a href="/Reg/EmpAdmin"> Employees </a>
<a href="/Reg/Login"> Logout</a>
<br />
<br />
<br />
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Quantity</th>
        <th>Preserve</th>
        <th>Status</th>
        <th>Assigned to</th>
        <th>Resturant ID</th>

        
    </tr>
    @foreach (var or in Model)
    {
        if (or.status == "Pickup Requested")
        {

            <tr>
                <td>@or.id</td>
                <td>@or.name</td>
                <td>@or.quantity</td>
                <td>@or.preserve</td>
                <td>@or.status</td>
                <td>@or.assign</td>
                <td>@or.restaurant_id</td>
             
            </tr>
        }
    }

</table>

