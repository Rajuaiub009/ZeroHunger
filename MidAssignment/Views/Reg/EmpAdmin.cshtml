
@{
    ViewBag.Title = "EmpAdmin";
}

<h3><u>Admin Dashboard</u></h3>
<a href="/Reg/AdminDashboard">Requests</a>
<a href="/Reg/AdminHistory"> History </a>
<a href="/Reg/AdminRes"> Restaurants </a>
<a href="/Reg/EmpAdmin"> Employees </a>
<a href="/Reg/Login"> Logout</a>
<br />
<br />
<br />
<table class="table table-bordered">
    @foreach (var t in Model)
    {
        if (t.type == "Employee")
        {
            <tr>
                <td>@t.id</td>
                <td>@t.name</td>
                <td>@t.contact</td>
                <td>@t.address</td>
                <td>@t.email</td>
                <td>@t.password</td>
                <td>@t.type</td>
            </tr>

        }

    }

</table>

