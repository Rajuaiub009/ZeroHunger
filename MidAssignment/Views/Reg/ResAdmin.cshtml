
@{
    ViewBag.Title = "ResAdmin";
}

<h3><u>Admin Dashboard</u></h3>
<a href="/Reg/AdminDashboard">Requests</a>
<a href="/Reg/AdminHistory"> History </a>
<a href="/Reg/ResAdmin"> Restaurants </a>
<a href="/Reg/EmpAdmin"> Employees </a>
<a href="/Reg/Login"> Logout</a>
<br />
<br />
<br />
<table class="table table-bordered">
    @foreach (var st in Model)
    {
        if (st.type == "Restaurant")
        {
            <tr>
                <td>@st.id</td>
                <td>@st.name</td>
                <td>@st.contact</td>
                <td>@st.address</td>
                <td>@st.email</td>
                <td>@st.password</td>
                <td>@st.type</td>
            </tr>

        }

    }

</table>


